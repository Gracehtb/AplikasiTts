<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CrudDosen">

    <EditText
        android:id="@+id/etFoto"
        android:layout_width="144dp"
        android:layout_height="38dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="304dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toEndOf="@+id/textNidn"
        app:layout_constraintTop_toBottomOf="@+id/namadosen" />

    <TextView
        android:id="@+id/textFoto"
        android:layout_width="53dp"
        android:layout_height="37dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="332dp"
        android:text="Foto"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textNama" />

    <TextView
        android:id="@+id/textGelar"
        android:layout_width="48dp"
        android:layout_height="39dp"
        android:layout_marginTop="304dp"
        android:layout_marginEnd="12dp"
        android:text="Gelar"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/namadosen"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etGelar"
        android:layout_width="244dp"
        android:layout_height="48dp"
        android:layout_marginTop="296dp"
        android:layout_marginEnd="84dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etEmail"
        android:layout_width="246dp"
        android:layout_height="49dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="156dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toEndOf="@+id/textNidn"
        app:layout_constraintTop_toBottomOf="@+id/namadosen" />

    <TextView
        android:id="@+id/textEmail"
        android:layout_width="51dp"
        android:layout_height="32dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="204dp"
        android:text="Email"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textNama" />

    <TextView
        android:id="@+id/textAlamat"
        android:layout_width="55dp"
        android:layout_height="25dp"
        android:layout_marginTop="168dp"
        android:layout_marginEnd="8dp"
        android:text="Alamat"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/namadosen"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etAlamat"
        android:layout_width="250dp"
        android:layout_height="43dp"
        android:layout_marginTop="160dp"
        android:layout_marginEnd="88dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textNama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:text="Nama"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/namadosen"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/namadosen"
        android:layout_width="236dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="104dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textNidn"
        android:layout_width="51dp"
        android:layout_height="22dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="64dp"
        android:text="NIDN"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textNama" />

    <EditText
        android:id="@+id/etNidn"
        android:layout_width="245dp"
        android:layout_height="47dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="24dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toEndOf="@+id/textNidn"
        app:layout_constraintTop_toBottomOf="@+id/namadosen" />

    <Button
        android:id="@+id/browseDsn"
        android:layout_width="86dp"
        android:layout_height="31dp"
        android:layout_marginTop="388dp"
        android:layout_marginEnd="52dp"
        android:text="browse...."
        android:textSize="8sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/saveDosen"
        android:layout_width="284dp"
        android:layout_height="38dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:text="SIMPAN"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etFoto" />
</androidx.constraintlayout.widget.ConstraintLayout>