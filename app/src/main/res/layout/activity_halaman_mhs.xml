<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HalamanMhs">

    <ImageButton
        android:id="@+id/datMhs"
        android:layout_width="115dp"
        android:layout_height="107dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="44dp"
        android:scaleType="fitCenter"
        android:onClick="DataDiri"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/datadiri" />

    <ImageButton
        android:id="@+id/dafKrs"
        android:layout_width="111dp"
        android:layout_height="105dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="44dp"
        android:scaleType="fitCenter"
        android:onClick="DaftarKrs"
        app:layout_constraintStart_toEndOf="@+id/datMhs"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/krs" />

    <ImageButton
        android:id="@+id/dafKelas"
        android:layout_width="119dp"
        android:layout_height="106dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="44dp"
        android:scaleType="fitCenter"
        android:onClick="LihatKelas"
        app:layout_constraintStart_toEndOf="@+id/dafKrs"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/kelaslihat" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="20dp"
        android:text="DATA DIRI"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/datMhs" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="111dp"
        android:layout_height="22dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="8dp"
        android:text="DAFTAR KRS"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/dafMhs"
        app:layout_constraintStart_toEndOf="@+id/textView15"
        app:layout_constraintTop_toBottomOf="@+id/dafKrs"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="121dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:text="LIHAT KELAS"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/textView16"
        app:layout_constraintTop_toBottomOf="@+id/dafKelas" />
</androidx.constraintlayout.widget.ConstraintLayout>